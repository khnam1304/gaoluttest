
@model List<ASP.Web.Models.GalleryModel>
@if (Model != null) {
    <ul class="list-gallery">
        @foreach (GalleryModel item in Model)
        {
            for (int i = 0; i <= item.Sources.Count() - 1; i++)
            {
                <!-- start: item inner  -->
                <li class="item">
                    <div class="box-images">
                        <a href="#">
                            <img alt="@(item.Sources[i].Alt ?? "default")" src="@ImageHelper.GetResize(item.Sources[i].Src, 88, 66)">
                        </a>
                    </div>
                </li>
                <!-- end: item inner  -->
            }
        }
    </ul>
    <!-- end: owl carousel  -->
}