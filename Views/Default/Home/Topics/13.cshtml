@model ASP.Web.Model.Models.Page
@Styles.Render("~/bundles/content/default/home/css")
@if (Model.StylesInsert != null)
{
    @(Model.StylesInsert.Contains("<style>") ? Html.Raw(Model.StylesInsert) : Html.Raw("<style>" + Model.StylesInsert + "</style>"))
}
<section class="as-content">
    <div id="main"><section class="banner"><div class="section"><div class="container-fluid"><div class="row"><div class="col-sm-12 p-0"><div class="rev_slider fullscreenbanner" id="rev_slider">@Html.Action("Album", "Widgets", new { identifier = "home-slider", pathView = "Album/Home", noWrapper = true, area = "" })</div></div></div></div></div></section><section class="features-categories"><div class="section mtn-10 mb-3"><div class="container-fluid"><div class="row"><div class="col-sm-12"><div class="category-carousel" data-auto-play="false" data-desktop="4" data-laptop="3" data-mobile="2" data-tablet="3">@Html.Action("CategoriesSpecial", "Catalog", new { groupOf = "vegetables,fruits,dried,juice,", pathView = "Widgets/Home/CategoriesSpecial", area = "" })</div></div></div></div></div></section><section class="core-value"><div class="section mt-6"><div class="container"><div class="row"><div class="col-sm-12"><div class="text-center mb-4 section-pretitle">Organikへようこそ</div><h3 class="text-center section-title mtn-2 mt-5">お客様にお届けする価値</h3><div class="organik-seperator mb-9 center"><div class="sep-icon"> </div></div></div></div></div></div><div class="section mb-5"><div class="container"><div class="row"><div class="col-sm-4"><div class="organik-about"><div class="image style-2" data-bg-color="#f5e9e2"><img alt="" class="img-responsive" src="../assets/organik/images/about/about_4.jpg"><div class="organik-about-letter">w</div></div><div class="content"><h5>私たちは誰ですか？</h5><p>現代の農業の時に有害な方法を理解して、私達は育てられる良質の有機農産物のためのニッチを始めました。</p><a class="link" href="/en/about-us.html">続きを読む </a></div></div></div><div class="col-sm-4"><div class="organik-about"><div class="image style-2" data-bg-color="#e7f2d8"><img alt="" class="img-responsive" src="../assets/organik/images/about/about_5.jpg"><div class="organik-about-letter">h</div></div><div class="content"><h5>どうやって動くの？</h5><p>私たちは、本物の季節の食材を楽しんで、土地とつながりを保つことを私たちのコミュニティに鼓舞するための最善の方法であると考えています。</p><a class="link" href="/en/about-us.html">続きを読む </a></div></div></div><div class="col-sm-4"><div class="organik-about"><div class="image style-2" data-bg-color="#f6edd0"><img alt="" class="img-responsive" src="../assets/organik/images/about/about_6.jpg"><div class="organik-about-letter">w</div></div><div class="content"><h5>私たちは何を育てますか？</h5><p>私たちは特別な注意を払い、特別な風味が焦点となっている特定の作物品種を選択して育てます。</p><a class="link" href="/en/about-us.html">続きを読む </a></div></div></div></div></div></div></section><section class="our-process"><div class="section bg-light pt-10 pb-10"><div class="container"><div class="row"><div class="col-sm-12"><div class="text-center mb-1 section-pretitle">自然食品</div><h2 class="text-center section-title mtn-2">配送プロセス</h2><div class="organik-seperator center mb-3"><div class="sep-icon"> </div></div><div class="organik-steps"><div class="step"><div class="step-icon"><i class="organik-lemon"></i><span class="order">1</span></div><div class="step-title">ステップ 01:</div><div class="step-text">製品を選択してください</div></div><div class="step-line"> </div><div class="step"><div class="step-icon"><i class="organik-barn"></i><span class="order">2</span></div><div class="step-title">ステップ 02:</div><div class="step-text">最寄りの農場をつなぐ</div></div><div class="step-line"> </div><div class="step"><div class="step-icon"><i class="organik-map-location"></i><span class="order">3</span></div><div class="step-title">ステップ 03:</div><div class="step-text">あなたの場所を共有する</div></div><div class="step-line"> </div><div class="step"><div class="step-icon"><i class="organik-delivery"></i><span class="order">4</span></div><div class="step-title">ステップ 04:</div><div class="step-text">早く配達する</div></div></div><div class="text-center"><a class="organik-btn brown mt-4" href="#">今すぐ始める</a></div></div></div></div></div></section><section class="out-product"><div class="section pt-12 pb-9"><div class="container"><div class="row"><div class="col-sm-12"><div class="text-center mb-1 section-pretitle">発見</div><h2 class="text-center section-title mtn-2">当社の製品</h2><div class="organik-seperator center"><div class="sep-icon"> </div></div></div></div>@Html.Action("CategoriesAll", "Catalog", new { pathView = "Widgets/Home/CategoriesAll", area = "" })</div></div></section><section class="banner-adv"><div class="section"><div class="container-fluid"><div class="row">@Html.Action("Album", "Widgets", new { identifier = "home-summer-season", pathView = "Album/home-summer-season", noWrapper = true, area = "" })</div></div></div></section><section class="features-product"><div class="section mt-16"><div class="container-fluid"><div class="row">@Html.Action("Album", "Widgets", new { identifier = "home-discover-banner", pathView = "Album/home-discover-banner", noWrapper = true, area = "" })<div class="col-md-8 col-sm-12 p-0"><div class="text-center"><div class="text-center mb-1 section-pretitle fz-34">発見</div><h2 class="text-center section-title mtn-2 fz-24">おすすめ商品</h2><div class="product-carousel" data-auto-play="true" data-desktop="3" data-laptop="2" data-mobile="1" data-tablet="2">@Html.Action("ProductFocus", "Catalog", new { pathView = "Widgets/Home/ProductFocus", area = "" })</div></div></div></div></div></div><div class="section"><div class="container"><div class="row"><div class="col-sm-12"><hr class="mt-8 mb-4"></div></div></div></div></section><section class="out-testimonial"><div class="section border-bottom pt-11 pb-10"><div class="container"><div class="row"><div class="col-sm-12"><div class="text-center mb-1 section-pretitle">私達の顧客</div><h2 class="text-center section-title mtn-2">私たちは素晴らしい農家です</h2><div class="organik-seperator center mb-8"><div class="sep-icon"> </div></div></div></div><div class="row">@Html.Action("TestimonialSpecial", "Widgets", new { type = "latest", area = "" })</div></div></div></section><section class="our-blog"><div class="section bg-light pt-12 pb-12"><div class="container"><div class="row"><div class="col-sm-12"><div class="text-center mb-1 section-pretitle">最新</div><h2 class="text-center section-title mtn-2">私たちのブログから</h2><div class="organik-seperator center mb-6"><div class="sep-icon"> </div></div></div></div><div class="row">@Html.Action("LatestPosts", "Widgets", new { grouped = "news", groupedForced = "news-featuring", take = 3, pathView = "Blog/LatestPostsIndex", area = "" })</div></div></div></section><div class="section pt-2 pb-4"><div class="container"><div class="row"><div class="col-sm-12"><div class="client-carousel" data-auto-play="true" data-desktop="5" data-laptop="3" data-mobile="2" data-tablet="3">@Html.Action("Album", "Widgets", new { identifier = "shared::clients-partners", pathView = "Album/ClientsPartners", noWrapper = true, area = "" })</div></div></div></div></div></div>
</section>
@section scripts{
    @Scripts.Render("~/bundles/content/default/home/js")
    @if (Model.ScriptsInsert != null)
    {
        @(Model.ScriptsInsert.Contains("<script>") ? Html.Raw("" + Model.ScriptsInsert + "") : Html.Raw("<script>" + Model.ScriptsInsert + "</script>"))
    }
}

