@using global::Web.Helpers
@model ASP.Web.Model.Models.Page
@Styles.Render("~/bundles/content/default/page/css")
@if (Model.StylesInsert != null)
{
    @(Model.StylesInsert.Contains("<style>") ? Html.Raw(Model.StylesInsert) : Html.Raw("<style>" + Model.StylesInsert + "</style>"))
}
    <div id="main">
        <div class="section border-bottom pt-2 pb-2">
            <div class="intro">
                <div class="intro-text">
                    <div class="container inner-wrap">
                        <h1>Hi there!<br> You look nice today.</h1>
                    </div>
                </div>
            </div>
        </div>
        <section class="as-content">
            <!-- Main Content -->

            <div id="content" class="contact-template">

                <div id="contact-teaser-holder">

                    <div class="container inner-wrap clearfix">

                        <div class="intro">

                            <p>We&#8217;d love to talk to you about Yoursites, or help you if you&#8217;ve got a problem. If you want to send us your questions, pictures of cats or even hand-written love notes like it&#8217;s 1985, you can find all our contact information here.</p>

                        </div>

                        <div class="floating-content-box">

                            <div class="block">
                                <h2>Social</h2>
                                <p>Follow us for quick updates, links and the odd joke on our very own social media pages. Additionally, use these to get in touch with us:</p>

                                <div class="social-buttons button-holder clearfix">

                                    <a href="https://www.facebook.com/yoursites" target="_blank" class="facebook external" rel="nofollow">
                                        <span>Pagelanes on Facebook</span>
                                    </a>

                                    <a href="https://www.twitter.com/yoursites" target="_blank" class="twitter external" rel="nofollow">
                                        <span>Pagelanes on Twitter</span>
                                    </a>

                                </div>

                            </div>

                            <div class="block">
                                <h2>Direct &amp; Fast</h2>
                                <p>If you&#8217;re looking to try our app, but want to talk to us first you can go ahead and chat live with us (it&#8217;s really us, promise) by following this link:</p>

                                <div class="direct-contact-button button-holder">
                                    <a href="javascript:Intercom('show');" class="">
                                        <span>Click here, to chat with us right now.</span>
                                    </a>
                                </div>

                            </div>

                        </div>

                    </div>

                </div>


                <div class="container inner-wrap clearfix">

                    <div class="old-school-holder">

                        <h2>Head Office</h2>
                        <p>If you&#8217;re in the neighborhood and lost or want to sent us postcards from your recent trips; or actually need to send documents, this is our physical place of business:</p>
                        <p>
                            <strong>ClubDesign M.P. GmbH</strong><br />
                            Dep. Pagelanes<br />
                            Lendkai 31<br />
                            8020 Graz<br />
                            Austria
                        </p>

                    </div>

                </div>


            </div>
            <div style="background: #dfe3e6;">

                <div class="container">
                    <div class="">
                        <div class="row">
                            <!-- Map Form -->
                            <div class="col-md-12 mt-md-0 ">
                                <div id="content">
                                    <div id="left">
                                        <div id="map">
                                            <p style="color:#f40">Where To Find Us</p>
                                            <div class="map-locator">
                                                <div class="tooltip">
                                                    <ul>
                                                        <li>
                                                            <span class="entypo-location"></span>
                                                            <span class="selectedLocation"><svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="feather"><polygon points="1 6 1 22 8 18 16 22 23 18 23 2 16 6 8 2 1 6"></polygon><line x1="8" y1="2" x2="8" y2="18"></line><line x1="16" y1="6" x2="16" y2="22"></line></svg> Số 489 Hoàng Quốc Việt, Cầu Giấy, HN</span>
                                                        </li>
                                                        <li>
                                                            <span class="entypo-mail"></span>
                                                            <a href="#"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-mail"><path d="M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z"></path><polyline points="22,6 12,13 2,6"></polyline></svg>  info@donavi.com.vn</a>
                                                        </li>
                                                        <li>
                                                            <span class="entypo-phone"></span>
                                                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-phone"><path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path></svg> Tel: 024-35567889 | Hotline: 0945 062 611
                                                        </li>
                                                    </ul>
                                                </div>
                                            </div>
                                            <div class="zoom"></div>
                                        </div>
                                        <ul id="location-bar">
                                            <li class="active">
                                                <a class="location " data-location="Số 489 Hoàng Quốc Việt, Cầu Giấy, HN" href="~/assets/images/location/head-office.png">Head Office</a>
                                            </li>
                                            <li>
                                                <a class="location" data-location="HN, Updating..." href="~/assets/images/location/office-2.png">Office Ha Noi</a>
                                            </li>
                                            <li>
                                                <a class="location" data-location="DN, Updating..." href="~/assets/images/location/office-1.png">Office Da Nang</a>
                                            </li>
                                            <li>
                                                <a class="location" data-location="HCM, Updating..." href="~/assets/images/location/office-2.png">Office HCM</a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <!-- /Map  Form -->

                        </div>
                    </div>
                </div>


            </div>

            <div class="tf-slider-left-right-shadow-2" style="margin-bottom: 50px;">


                <div class="container card">
                    <div class="card-body">
                        <div class="row">
                            <!-- Contact Us Form -->
                            <div class="col-md-12 mt-md-0 ">
                                @{ Html.RenderPartial("~/Views/Default/Widgets/FeedBack/_FormPatial.cshtml", new FeedBack() { Type = "contact-us" });}
                            </div>
                            <!-- /Contact Us Form -->

                        </div>
                    </div>
                </div>

            </div>

            <!-- /Main Content -->

        </section>
    </div>
@section scripts{
    @Scripts.Render("~/bundles/content/default/page/js")
    @if (Model.ScriptsInsert != null)
        {
        @(Model.ScriptsInsert.Contains("<script>") ? Html.Raw("" + Model.ScriptsInsert + "") : Html.Raw("<script>" + Model.ScriptsInsert + "</script>"))
    }
} 
